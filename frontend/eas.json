{
  "cli": {
    "version": ">= 13.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.19.4",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": false,
      "node": "20.19.4",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_USE_DEV_CLIENT": "false"
      }
    },
    "production": {
      "developmentClient": false,
      "node": "20.19.4",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_USE_DEV_CLIENT": "false"
      }
    },
    "preview-apk": {
      "extends": "production",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "config": {
          "packageManager": "yarn"
        }
      },
      "env": {
        "APP_VARIANT": "release",
        "EXPO_NO_DEV_CLIENT": "1"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
